        "run:ios:uat:debug": "react-native run-ios --scheme 'M2ULife UAT' --simulator='iPhone 12 Pro Max'",
        "run:ios:dev:debug": "react-native run-ios --scheme 'M2ULife Development' --simulator='iPhone 12 Pro Max'",
